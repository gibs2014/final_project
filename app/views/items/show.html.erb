<div class="page-header">
  <h1>Item #<%= @item.id %></h1>
</div>

<dl class="dl-horizontal">
  <dt>Name</dt>
  <dd><%= @item.name %></dd>

  <dt>Category</dt>
  <dd><%= @item.category_id %></dd>

  <dt>Color</dt>
  <dd><%= @item.color %></dd>

  <dt>Brand</dt>
  <dd><%= @item.brand %></dd>

  <dt>Price</dt>
  <dd><%= @item.price %></dd>

</dl>
